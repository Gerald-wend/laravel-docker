#!/usr/bin/env bash


if [ ! -d /.composer ]; then
    mkdir /.composer
fi

chmod -R ugo+rw /.composer

FILE=/var/www/html/setup.sh
if test -f "$FILE"; then
    ./var/www/html/setup.sh
fi

service cron start

if [ $# -gt 0 ];then
    exec gosu $WWWUSER "$@"
else
    /usr/bin/supervisord -c /etc/supervisor/conf.d/supervisord.conf
fi

